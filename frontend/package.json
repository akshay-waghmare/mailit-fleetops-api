    "build:all": "ng build console-app",
    "serve:prod": "ng serve console-app --configuration production",
    "analyze": "ng build console-app --analyze"
  },
  "dependencies": {
    "@angular/animations": "^20.2.0",
    "@angular/cdk": "^20.2.0",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/material": "^20.2.0",
    "@angular/platform-browser": "^20.2.0",
    "@angular/platform-browser-dynamic": "^20.2.0",
    "@angular/platform-server": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@angular/ssr": "^20.2.0",
    "@maplibre/maplibre-gl-geocoder": "^1.7.0",
    "express": "^5.1.0",
    "maplibre-gl": "^4.7.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
