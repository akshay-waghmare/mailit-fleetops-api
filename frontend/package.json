{
  "name": "fleetops-frontend",
  "version": "1.0.0",
  "description": "FleetOps Angular Frontend Workspace",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "ng build console-app",
    "serve": "ng serve console-app --port 4200",
