{"ast":null,"code":"import { UIKitCardComponent } from './ui-kit-card.component';\nexport default {\n  title: 'UI Kit/Card',\n  component: UIKitCardComponent,\n  parameters: {\n    layout: 'centered'\n  },\n  argTypes: {\n    variant: {\n      control: 'select',\n      options: ['default', 'elevated', 'bordered', 'interactive', 'vehicle', 'route', 'order', 'alert']\n    },\n    hasHeader: {\n      control: 'boolean'\n    },\n    hasFooter: {\n      control: 'boolean'\n    }\n  }\n};\nexport const Default = {\n  args: {\n    variant: 'default',\n    hasHeader: false,\n    hasFooter: false\n  },\n  render: args => ({\n    props: args,\n    template: `\n      <ui-card [variant]=\"variant\" [hasHeader]=\"hasHeader\" [hasFooter]=\"hasFooter\">\n        <p>This is a basic card with some content. Cards are great for displaying related information in a organized way.</p>\n      </ui-card>\n    `\n  })\n};\nexport const WithHeaderAndFooter = {\n  args: {\n    variant: 'default',\n    hasHeader: true,\n    hasFooter: true\n  },\n  render: args => ({\n    props: args,\n    template: `\n      <ui-card [variant]=\"variant\" [hasHeader]=\"hasHeader\" [hasFooter]=\"hasFooter\">\n        <div slot=\"header\">\n          <h3 style=\"margin: 0; color: #2196f3;\">Card Title</h3>\n        </div>\n        <p>This card has both a header and footer section. The header can contain titles, while the footer can have actions or additional information.</p>\n        <div slot=\"footer\">\n          <button style=\"background: #2196f3; color: white; border: none; padding: 8px 16px; border-radius: 4px; cursor: pointer;\">Action</button>\n        </div>\n      </ui-card>\n    `\n  })\n};\nexport const FleetManagementCards = {\n  render: () => ({\n    template: `\n      <div style=\"display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); gap: 16px; width: 100%; max-width: 1200px;\">\n        \n        <!-- Vehicle Card -->\n        <ui-card variant=\"vehicle\" [hasHeader]=\"true\">\n          <div slot=\"header\">\n            <h4 style=\"margin: 0; color: #4caf50;\">üöõ Vehicle FL-001</h4>\n          </div>\n          <div>\n            <p><strong>Status:</strong> En Route</p>\n            <p><strong>Driver:</strong> John Smith</p>\n            <p><strong>Current Location:</strong> Highway 101</p>\n            <p><strong>ETA:</strong> 2:30 PM</p>\n          </div>\n        </ui-card>\n\n        <!-- Route Card -->\n        <ui-card variant=\"route\" [hasHeader]=\"true\">\n          <div slot=\"header\">\n            <h4 style=\"margin: 0; color: #2196f3;\">üó∫Ô∏è Route R-2024-001</h4>\n          </div>\n          <div>\n            <p><strong>Distance:</strong> 145 km</p>\n            <p><strong>Stops:</strong> 8</p>\n            <p><strong>Estimated Time:</strong> 3h 20m</p>\n            <p><strong>Progress:</strong> 65% Complete</p>\n          </div>\n        </ui-card>\n\n        <!-- Order Card -->\n        <ui-card variant=\"order\" [hasHeader]=\"true\">\n          <div slot=\"header\">\n            <h4 style=\"margin: 0; color: #ff9800;\">üì¶ Order #ORD-2024-1205</h4>\n          </div>\n          <div>\n            <p><strong>Customer:</strong> ABC Corp</p>\n            <p><strong>Items:</strong> 15 packages</p>\n            <p><strong>Priority:</strong> High</p>\n            <p><strong>Delivery:</strong> Today 4:00 PM</p>\n          </div>\n        </ui-card>\n\n        <!-- Alert Card -->\n        <ui-card variant=\"alert\" [hasHeader]=\"true\">\n          <div slot=\"header\">\n            <h4 style=\"margin: 0; color: #f44336;\">‚ö†Ô∏è System Alert</h4>\n          </div>\n          <div>\n            <p><strong>Type:</strong> Vehicle Breakdown</p>\n            <p><strong>Vehicle:</strong> FL-003</p>\n            <p><strong>Location:</strong> Downtown Depot</p>\n            <p><strong>Action Required:</strong> Immediate</p>\n          </div>\n        </ui-card>\n\n      </div>\n    `\n  })\n};\nexport const InteractiveCards = {\n  render: () => ({\n    template: `\n      <div style=\"display: flex; gap: 16px; flex-wrap: wrap;\">\n        \n        <ui-card variant=\"interactive\">\n          <h4 style=\"margin-top: 0; color: #2196f3;\">Click Me!</h4>\n          <p>This card is interactive and shows hover effects.</p>\n        </ui-card>\n\n        <ui-card variant=\"elevated\">\n          <h4 style=\"margin-top: 0; color: #9c27b0;\">Elevated</h4>\n          <p>This card has an elevated shadow effect.</p>\n        </ui-card>\n\n        <ui-card variant=\"bordered\">\n          <h4 style=\"margin-top: 0; color: #2196f3;\">Bordered</h4>\n          <p>This card has a prominent border.</p>\n        </ui-card>\n\n      </div>\n    `\n  })\n};","map":{"version":3,"names":["UIKitCardComponent","title","component","parameters","layout","argTypes","variant","control","options","hasHeader","hasFooter","Default","args","render","props","template","WithHeaderAndFooter","FleetManagementCards","InteractiveCards"],"sources":["E:/Project/mailit-postal-project-backend/frontend/apps/console-app/src/stories/ui-kit-card.stories.ts"],"sourcesContent":["import { UIKitCardComponent } from './ui-kit-card.component';\nexport default {\n    title: 'UI Kit/Card',\n    component: UIKitCardComponent,\n    parameters: {\n        layout: 'centered',\n    },\n    argTypes: {\n        variant: {\n            control: 'select',\n            options: ['default', 'elevated', 'bordered', 'interactive', 'vehicle', 'route', 'order', 'alert'],\n        },\n        hasHeader: {\n            control: 'boolean',\n        },\n        hasFooter: {\n            control: 'boolean',\n        },\n    },\n};\nexport const Default = {\n    args: {\n        variant: 'default',\n        hasHeader: false,\n        hasFooter: false,\n    },\n    render: (args) => ({\n        props: args,\n        template: `\r\n      <ui-card [variant]=\"variant\" [hasHeader]=\"hasHeader\" [hasFooter]=\"hasFooter\">\r\n        <p>This is a basic card with som